{\n  \"$schema\": \"http://json-schema.org/draft-07/schema#\",\n  \"title\": \"EyeCore Main Data Structure\",\n  \"description\": \"Complete EyeCore data collection - collected every 5 seconds\",\n  \"type\": \"object\",\n  \"required\": [\n    \"session_id\",\n    \"timestamp\",\n    \"system_metrics\",\n    \"process_data\",\n    \"input_metrics\",\n    \"network_metrics\",\n    \"focus_metrics\"\n  ],\n  \"properties\": {\n    \"session_id\": {\n      \"type\": \"string\",\n      \"format\": \"uuid\",\n      \"description\": \"Unique session identifier (UUID v4)\",\n      \"example\": \"a1b2c3d4-e5f6-7890-abcd-ef1234567890\"\n    },\n    \"timestamp\": {\n      \"type\": \"string\",\n      \"format\": \"date-time\",\n      \"description\": \"UTC timestamp of data collection (ISO 8601)\",\n      \"example\": \"2025-10-25T21:26:11Z\"\n    },\n    \"system_metrics\": {\n      \"$ref\": \"./component_schemas/SystemMetrics.schema.json\"\n    },\n    \"process_data\": {\n      \"$ref\": \"./component_schemas/ProcessData.schema.json\"\n    },\n    \"input_metrics\": {\n      \"$ref\": \"./component_schemas/InputMetrics.schema.json\"\n    },\n    \"network_metrics\": {\n      \"$ref\": \"./component_schemas/NetworkMetrics.schema.json\"\n    },\n    \"focus_metrics\": {\n      \"$ref\": \"./component_schemas/FocusMetrics.schema.json\"\n    },\n    \"voice_data\": {\n      \"description\": \"Voice analysis data (optional, opt-in collection)\",\n      \"oneOf\": [\n        {\"$ref\": \"./component_schemas/VoiceData.schema.json\"},\n        {\"type\": \"null\"}\n      ]\n    },\n    \"camera_data\": {\n      \"description\": \"Camera/facial emotion data (optional, opt-in collection)\",\n      \"oneOf\": [\n        {\"$ref\": \"./component_schemas/CameraData.schema.json\"},\n        {\"type\": \"null\"}\n      ]\n    },\n    \"keystroke_dynamics\": {\n      \"description\": \"Typing pattern analysis (optional, opt-in collection)\",\n      \"oneOf\": [\n        {\"$ref\": \"./component_schemas/KeystrokeDynamics.schema.json\"},\n        {\"type\": \"null\"}\n      ]\n    },\n    \"screen_interactions\": {\n      \"description\": \"UI interaction data (optional)\",\n      \"oneOf\": [\n        {\"$ref\": \"./component_schemas/ScreenInteractions.schema.json\"},\n        {\"type\": \"null\"}\n      ]\n    },\n    \"file_metadata\": {\n      \"description\": \"File access patterns (optional, opt-in collection)\",\n      \"oneOf\": [\n        {\"$ref\": \"./component_schemas/FileMetadata.schema.json\"},\n        {\"type\": \"null\"}\n      ]\n    },\n    \"system_events\": {\n      \"description\": \"Power and peripheral events (optional)\",\n      \"oneOf\": [\n        {\"$ref\": \"./component_schemas/SystemEvents.schema.json\"},\n        {\"type\": \"null\"}\n      ]\n    },\n    \"mouse_dynamics\": {\n      \"description\": \"Mouse movement analytics (optional)\",\n      \"oneOf\": [\n        {\"$ref\": \"./component_schemas/MouseDynamics.schema.json\"},\n        {\"type\": \"null\"}\n      ]\n    },\n    \"network_activity_metadata\": {\n      \"description\": \"Enhanced network inference data (optional)\",\n      \"oneOf\": [\n        {\"$ref\": \"./component_schemas/NetworkActivityMetadata.schema.json\"},\n        {\"type\": \"null\"}\n      ]\n    }\n  },\n  \"additionalProperties\": false,\n  \"examples\": [\n    {\n      \"session_id\": \"a1b2c3d4-e5f6-7890-abcd-ef1234567890\",\n      \"timestamp\": \"2025-10-25T21:26:11Z\",\n      \"system_metrics\": {\n        \"timestamp\": \"2025-10-25T21:26:11Z\",\n        \"cpu_usage\": 45.2,\n        \"memory_usage\": 62.8,\n        \"disk_usage\": 78.5\n      },\n      \"process_data\": {\n        \"timestamp\": \"2025-10-25T21:26:11Z\",\n        \"active_process\": \"0x12345678\",\n        \"active_window_title\": \"Visual Studio Code\",\n        \"process_count\": 156\n      },\n      \"input_metrics\": {\n        \"timestamp\": \"2025-10-25T21:26:11Z\",\n        \"mouse_clicks\": 3,\n        \"keyboard_events\": 12,\n        \"idle_duration_seconds\": 2\n      },\n      \"network_metrics\": {\n        \"timestamp\": \"2025-10-25T21:26:11Z\",\n        \"bytes_sent\": 524288,\n        \"bytes_received\": 2097152,\n        \"active_connections\": 18\n      },\n      \"focus_metrics\": {\n        \"timestamp\": \"2025-10-25T21:26:11Z\",\n        \"focus_level\": 0.75,\n        \"context_switches\": 8,\n        \"productive_app_time\": 420\n      },\n      \"voice_data\": null,\n      \"camera_data\": null,\n      \"keystroke_dynamics\": null,\n      \"screen_interactions\": null,\n      \"file_metadata\": null,\n      \"system_events\": null,\n      \"mouse_dynamics\": null,\n      \"network_activity_metadata\": null\n    }\n  ]\n}\n